#! /bin/bash

if [ $1 == "all" -o $1 == "m32" ]
then
CLFAGS=-m32 CXXFLAGS=-m32 LDFLAGS=-m32 CROSS_COMPILE=no ARCH_TYPE=m32 OUTPUT_TYPE=.a OUTPUT_NAME=llwip DEBUG=0 make clean;
CLFAGS=-m32 CXXFLAGS=-m32 LDFLAGS=-m32 CROSS_COMPILE=no ARCH_TYPE=m32 OUTPUT_TYPE=.a OUTPUT_NAME=llwip DEBUG=0 make dir;
CLFAGS=-m32 CXXFLAGS=-m32 LDFLAGS=-m32 CROSS_COMPILE=no ARCH_TYPE=m32 OUTPUT_TYPE=.a OUTPUT_NAME=llwip DEBUG=0 make all;
fi

